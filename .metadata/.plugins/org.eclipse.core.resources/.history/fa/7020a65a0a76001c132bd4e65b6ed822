package primerospasos;

import javax.swing.JOptionPane;

public class V26_ArraysIV_ArraysBidimensionales {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double capital;
		double interes = 0.10;
		
		int agnos = Integer.parseInt(JOptionPane.showInputDialog("Cantidad de años"));
		
		double tabla_capital [][] = new double[6][agnos];
		
		for (int i = 0; i < 6; i++) {
			capital = 10000;
			for (int j = 0; j < agnos; j++) {
				tabla_capital[i][j] = capital;
				capital = capital * interes;
			}
			interes = interes + 0.01;
		}
		
	}

}
