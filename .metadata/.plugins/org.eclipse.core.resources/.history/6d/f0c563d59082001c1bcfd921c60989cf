package poo;

public class V028_POO_II_Coche {
	private int ruedas;
	private int largo;
	private int ancho;
	private int motor;
	private int peso;
	
	public V028_POO_II_Coche() {
		this.ruedas = 4;
		this.largo = 2000;
		this.ancho = 300;
		this.motor = 1600;
		this.peso = 500;
	}
	public String dime_largo(){
		return "El largo del coche es: " + largo;
	} 
}
